<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kr.teamcadi.myapa">
    <!-- 인터넷 연결 권한 등록 -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApaAndroid">
        <activity android:name=".activity.ReportAddActivity"></activity>
        <activity android:name=".activity.CalendarActivity" />
        <activity android:name=".activity.WritepageActivity" />
        <activity android:name=".activity.CommentActivity" />
        <activity android:name=".activity.FeedActivity" />
        <activity android:name=".activity.ReportActivity" />
        <activity android:name=".activity.SearchActivity" />
        <activity android:name=".activity.FindIdAndPwActivity" />
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>